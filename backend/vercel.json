{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
    "routes": [
      {
        "src": "/api/users",
        "dest": "./index.js" ,
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
      {
        "src": "/api/notes",
        "dest": "./index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      } 
      },
      {
        "src": "/api/users/:username",
        "dest": "./index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }  
      },
      {
        "src": "/api/notes/:id",
        "dest": "./index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }   
      }
    ]
  }
  
  